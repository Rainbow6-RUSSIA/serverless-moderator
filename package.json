{
  "name": "r6-mod",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/register.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"yarn start\"",
    "start": "NODE_ENV=development node ./dist/index.js",
    "tf:apply:dev": "cd infrastructure && terraform apply -var-file=\"../.env\"",
    "tf:plan:dev": "cd infrastructure && terraform plan -var-file=\"../.env\"",
    "tf:init:dev": "cd infrastructure && terraform init && terraform workspace select dev",
    "deploy:dev": "yarn zip && yarn git:clean-tree && yarn tf:init:dev && yarn tf:apply:dev -auto-approve && yarn register:dev",
    "register:dev": "yarn build && NODE_ENV=development node ./dist/register.js",
    "deploy:force": "yarn zip && yarn tf:apply:dev -auto-approve -replace=\"yandex_function.entrypoint\"",
    "git:clean-tree": "git diff --exit-code && git diff --cached --exit-code",
    "build": "tsc",
    "zip": "yarn clear && yarn build && node zip.js",
    "clear": "rm -rf dist",
    "tf:apply:prod": "cd infrastructure && terraform apply -var-file=\"../.env.production\"",
    "tf:plan:prod": "cd infrastructure && terraform plan -var-file=\"../.env.production\"",
    "tf:init:prod": "cd infrastructure && terraform init && terraform workspace select prod",
    "deploy:prod": "yarn zip && yarn git:clean-tree && yarn tf:init:prod && yarn tf:apply:prod -auto-approve && yarn register:prod",
    "register:prod": "yarn build && NODE_ENV=production node ./dist/register.js",
    "postinstall": "patch-package"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.2",
    "fs-extra": "^11.1.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@sapphire/pieces": "^3.7.0",
    "@sapphire/snowflake": "^3.5.1",
    "@skyra/http-framework": "^1.1.1",
    "discord-interactions": "^3.4.0",
    "discord.js": "14.13.0",
    "dotenv": "^16.3.1",
    "dynamoose": "^3.2.1",
    "express": "^5.0.0-beta.1",
    "morgan": "^1.10.0",
    "node-domexception": "1.x",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "serverless-http": "^3.2.0",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": "16.14.1",
    "yarn": "3"
  },
  "resolutions": {
    "discord-api-types": "0.37.56",
    "qs": "6.9.7"
  }
}
