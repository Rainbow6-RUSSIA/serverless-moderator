{
  "name": "r6-mod",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/register.js",
  "scripts": {
    "tf:apply": "cd infrastructure && terraform apply",
    "tf:plan": "cd infrastructure && terraform plan",
    "tf:init": "cd infrastructure && terraform init",
    "git:clean-tree": "git diff --exit-code && git diff --cached --exit-code",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "zip": "node zip.js",
    "clear": "rm -rf dist",
    "deploy": "yarn clear && yarn build && yarn zip && yarn git:clean-tree && yarn tf:apply -auto-approve",
    "deploy:force": "yarn clear && yarn build && yarn zip && yarn tf:apply  -auto-approve -replace=\"yandex_function.entrypoint\"",
    "register": "yarn build && node --experimental-specifier-resolution=node ./dist/register.js"
  },
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.6.2",
    "fs-extra": "^11.1.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/express": "^2.3.0",
    "@sapphire/pieces": "^3.7.0",
    "@sapphire/snowflake": "^3.5.1",
    "@skyra/http-framework": "^1.1.1",
    "discord-interactions": "^3.4.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "dynamoose": "^3.2.1",
    "fastify": "^4.23.2",
    "node-domexception": "1.x",
    "serverless-http": "^3.2.0"
  },
  "engines": {
    "node": "16.14.1",
    "yarn": "3"
  },
  "resolutions": {
    "discord-api-types": "0.37.56"
  }
}
